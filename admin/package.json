{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@antv/g2": "^4.0.12",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.4",
    "antd": "^4.3.4",
    "axios": "^0.20.0",
    "dayjs": "^1.9.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "re-editor": "^0.5.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-quill": "^1.3.5",
    "umi": "^3.2.4",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {}
}
